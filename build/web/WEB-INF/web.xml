<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>StaffList</servlet-name>
        <servlet-class>controller.StaffListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>detail</servlet-name>
        <servlet-class>controller.DetailControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>controller.StaffSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaffEditPack</servlet-name>
        <servlet-class>controller.StaffEditPack</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaffAddCliPck</servlet-name>
        <servlet-class>controller.AddPack</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaffEditTrainer</servlet-name>
        <servlet-class>controller.StaffEditTrainer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaffAddCustomer</servlet-name>
        <servlet-class>controller.StaffAddCustomer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StaffList</servlet-name>
        <url-pattern>/StaffList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>detail</servlet-name>
        <url-pattern>/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>search</servlet-name>
        <url-pattern>/StaffSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaffEditPack</servlet-name>
        <url-pattern>/StaffEditPack</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaffAddCliPck</servlet-name>
        <url-pattern>/StaffAddCliPck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaffEditTrainer</servlet-name>
        <url-pattern>/StaffEditTrainer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaffAddCustomer</servlet-name>
        <url-pattern>/StaffAddCustomer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
